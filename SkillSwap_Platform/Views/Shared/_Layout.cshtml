                                                            <button type="submit" class="btn btn-link text-decoration-none dropdown-item">
                                                                <i class="flaticon-logout mr10"></i> Logout
                                                            </button>
                                                        </form>
                                                    </div>
                                                </div>
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        }
