                                        <a asp-controller="Home" asp-action="ExternalLogin" asp-route-provider="Google" class="ud-btn btn-google fz14 fw400 mb-2 mb-md-0"><i class="fab fa-google"></i> Continue Google</a>
                                        <a asp-controller="Home" asp-action="ExternalLogin" asp-route-provider="Apple" class="ud-btn btn-apple fz14 fw400"><i class="fab fa-apple"></i> Continue Apple</a>
                                        <a asp-controller="Home" asp-action="ExternalLogin" asp-route-provider="GitHub" class="ud-btn btn-apple fz14 fw400"><i class="fab fa-gitHub"></i> Continue GitHub</a>
                                    </div> *@
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
