            <!-- Our LogIn Area -->
            <section class="our-login">
                <form asp-action="Login" method="post">
                    @Html.AntiForgeryToken()
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-6 m-auto wow fadeInUp" data-wow-delay="300ms">
                                <div class="main-title text-center">
                                    <h2 class="title">Log In</h2>
                                    <p class="paragraph">Welcome back, unlock new possibilities with every click.</p>
                                </div>
                            </div>
                        </div>
                        <div class="row wow fadeInRight" data-wow-delay="300ms">
                            <div class="col-xl-6 mx-auto">
                                <div class="log-reg-form search-modal form-style1 bgc-white p50 p30-sm default-box-shadow1 bdrs12">
                                    <div class="mb30">
                                        <h4>We're glad to see you again!</h4>
                                        <p class="text">Don't have an account? <a href="@Url.ActionLink("Register","Home")" class="text-thm">Sign Up!</a></p>
                                    </div>
                                    @if (TempData["ApprovalMessage"] != null)
                                    {
                                        <div class="message-alart-style1">
                                            <div class="alert alart_style_one alert-dismissible fade show mb20" role="alert">
                                                Info: @TempData["ApprovalMessage"]
                                                <i class="far fa-xmark btn-close" data-bs-dismiss="alert" aria-label="Close"></i>
                                            </div>
                                        </div>
                                    }
                                    @if (TempData["SuccessMessage"] != null)
                                    {
                                        <div class="message-alart-style1">
                                            <div class="alert alart_style_four alert-dismissible fade show mb20" role="alert">
                                                @TempData["SuccessMessage"]
                                                <i class="far fa-xmark btn-close" data-bs-dismiss="alert" aria-label="Close"></i>
                                            </div>
                                        </div>
                                    }
                                    @if (TempData["ErrorMessage"] != null)
                                    {
                                        <div class="message-alart-style1">
                                            <div class="alert alart_style_three alert-dismissible fade show mb20" role="alert">
                                                @TempData["ErrorMessage"]
                                                <i class="far fa-xmark btn-close" data-bs-dismiss="alert" aria-label="Close"></i>
                                            </div>
                                        </div>
                                    }
                                    <div class="mb20">
                                        <label class="form-label fw600 dark-color">Email Address</label>
                                        <input asp-for="LoginName" type="email" class="form-control" placeholder="johndoe58@gmail.com">
                                        <span asp-validation-for="LoginName" class="text-danger"></span>
                                    </div>
                                    <div class="mb15">
                                        <label class="form-label fw600 dark-color">Password</label>
                                        <input asp-for="Password" type="password" class="form-control" placeholder="*******">
                                        <span asp-validation-for="Password" class="text-danger"></span>
                                    </div>
                                    <div class="checkbox-style1 d-block d-sm-flex align-items-center justify-content-between mb20">
                                        <label class="custom_checkbox fz14 ff-heading">
                                            Remember me
                                            <input asp-for="RememberMe" type="checkbox" checked="checked">
                                            <span class="checkmark"></span>
                                        </label>
                                        <a class="fz14 ff-heading" href="@Url.ActionLink("ForgotPassword","Home")">Lost your password?</a>
                                    </div>

                                    <div class="d-grid mb20">
                                        <button class="ud-btn btn-thm" type="submit">Log In <i class="fal fa-arrow-right-long"></i></button>
                                    </div>
                                    @* <div class="hr_content mb20"><hr><span class="hr_top_text">OR</span></div>
                                    <div class="d-md-flex justify-content-center">
