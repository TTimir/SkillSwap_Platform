                    {
                        foreach (var freelancer in Model.HighestRatedFreelancers)
                        {
                            <div class="item">
                                <div class="freelancer-style1 text-center bdr1 bdrs16 hover-box-shadow">
                                    <div class="thumb w90 mb25 mx-auto position-relative rounded-circle">
